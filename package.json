{
  "name": "@ardinsys/financial-charts",
  "version": "0.9.8",
  "type": "module",
  "license": "Apache 2.0",
  "packageManager": "pnpm@10.24.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/ardinsys/financial-charts.git"
  },
  "author": "ARDINSYS Zrt.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "vite --host",
    "build": "vite build && tsc src/index.ts --declaration --emitDeclarationOnly --esModuleInterop --outDir dist --target es2022 --moduleResolution bundler",
    "preview": "vite preview",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@types/node": "^24.4.0",
    "@vitejs/plugin-vue": "^6.0.0",
    "prettier": "^3.6.2",
    "typescript": "~5.8.3",
    "vite": "^7.0.4",
    "vitepress": "^1.4.2",
    "vue": "^3.3.11",
    "vue-tsc": "^2.2.12"
  }
}
